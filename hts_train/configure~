if test -z "$version"; then
	{ { echo "$as_me:$LINENO: error: merge in SPTK version 3.1 or later is required." >&5
echo "$as_me: error: merge in SPTK version 3.1 or later is required." >&2;}
   { (exit 1); exit 1; }; }
elif test `echo "$version < 3.1" | $BC` = 1; then
	{ { echo "$as_me:$LINENO: error: merge in SPTK version 3.1 or later is required." >&5
echo "$as_me: error: merge in SPTK version 3.1 or later is required." >&2;}
