# mgcep
if test -z "$GAMMA"; then
	# Extract the first word of "mcep", so it can be a program name with args.
set dummy mcep; ac_word=$2
echo "$as_me:$LINENO: checking for $ac_word" >&5
echo $ECHO_N "checking for $ac_word... $ECHO_C" >&6
if test "${ac_cv_path_MGCEP+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  case $MGCEP in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_MGCEP="$MGCEP" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
as_dummy="${sptk_search_path}:$PATH"
for as_dir in $as_dummy
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
  for ac_exec_ext in '' $ac_executable_extensions; do
  if $as_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_path_MGCEP="$as_dir/$ac_word$ac_exec_ext"
    echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done

  test -z "$ac_cv_path_MGCEP" && ac_cv_path_MGCEP=":"
  ;;
esac
fi
MGCEP=$ac_cv_path_MGCEP

if test -n "$MGCEP"; then
  echo "$as_me:$LINENO: result: $MGCEP" >&5
echo "${ECHO_T}$MGCEP" >&6
else
  echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6
fi

	if test ! -f "$MGCEP"; then
		{ { echo "$as_me:$LINENO: error: Can't find mcep of SPTK" >&5
echo "$as_me: error: Can't find mcep of SPTK" >&2;}
   { (exit 1); exit 1; }; }
	fi
	version=`$MGCEP -h 2>&1 | grep "version" | sed -e "s/.* //g"`
	if test -z "$version"; then
		{ { echo "$as_me:$LINENO: error: mcep in SPTK version 3.2 or later is required." >&5
echo "$as_me: error: mcep in SPTK version 3.2 or later is required." >&2;}
   { (exit 1); exit 1; }; }
	elif test `echo "$version < 3.2" | $BC` = 1; then
		{ { echo "$as_me:$LINENO: error: mcep in SPTK version 3.2 or later is required." >&5
echo "$as_me: error: mcep in SPTK version 3.2 or later is required." >&2;}
   { (exit 1); exit 1; }; }
	fi
elif test $GAMMA -eq 0; then
	# Extract the first word of "mcep", so it can be a program name with args.
set dummy mcep; ac_word=$2
echo "$as_me:$LINENO: checking for $ac_word" >&5
echo $ECHO_N "checking for $ac_word... $ECHO_C" >&6
if test "${ac_cv_path_MGCEP+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  case $MGCEP in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_MGCEP="$MGCEP" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
as_dummy="${sptk_search_path}:$PATH"
for as_dir in $as_dummy
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
  for ac_exec_ext in '' $ac_executable_extensions; do
  if $as_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_path_MGCEP="$as_dir/$ac_word$ac_exec_ext"
    echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done

  test -z "$ac_cv_path_MGCEP" && ac_cv_path_MGCEP=":"
  ;;
esac
fi
MGCEP=$ac_cv_path_MGCEP

if test -n "$MGCEP"; then
  echo "$as_me:$LINENO: result: $MGCEP" >&5
echo "${ECHO_T}$MGCEP" >&6
else
  echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6
fi

	if test ! -f "$MGCEP"; then
		{ { echo "$as_me:$LINENO: error: Can't find mcep of SPTK" >&5
echo "$as_me: error: Can't find mcep of SPTK" >&2;}
   { (exit 1); exit 1; }; }
	fi
	version=`$MGCEP -h 2>&1 | grep "version" | sed -e "s/.* //g"`
	if test -z "$version"; then
		{ { echo "$as_me:$LINENO: error: mcep in SPTK version 3.2 or later is required." >&5
echo "$as_me: error: mcep in SPTK version 3.2 or later is required." >&2;}
   { (exit 1); exit 1; }; }
	elif test `echo "$version < 3.2" | $BC` = 1; then
		{ { echo "$as_me:$LINENO: error: mcep in SPTK version 3.2 or later is required." >&5
echo "$as_me: error: mcep in SPTK version 3.2 or later is required." >&2;}
   { (exit 1); exit 1; }; }
	fi
else
	# Extract the first word of "mgcep", so it can be a program name with args.
set dummy mgcep; ac_word=$2
echo "$as_me:$LINENO: checking for $ac_word" >&5
echo $ECHO_N "checking for $ac_word... $ECHO_C" >&6
if test "${ac_cv_path_MGCEP+set}" = set; then
  echo $ECHO_N "(cached) $ECHO_C" >&6
else
  case $MGCEP in
  [\\/]* | ?:[\\/]*)
  ac_cv_path_MGCEP="$MGCEP" # Let the user override the test with a path.
  ;;
  *)
  as_save_IFS=$IFS; IFS=$PATH_SEPARATOR
as_dummy="${sptk_search_path}:$PATH"
for as_dir in $as_dummy
do
  IFS=$as_save_IFS
  test -z "$as_dir" && as_dir=.
  for ac_exec_ext in '' $ac_executable_extensions; do
  if $as_executable_p "$as_dir/$ac_word$ac_exec_ext"; then
    ac_cv_path_MGCEP="$as_dir/$ac_word$ac_exec_ext"
    echo "$as_me:$LINENO: found $as_dir/$ac_word$ac_exec_ext" >&5
    break 2
  fi
done
done

  test -z "$ac_cv_path_MGCEP" && ac_cv_path_MGCEP=":"
  ;;
esac
fi
MGCEP=$ac_cv_path_MGCEP

if test -n "$MGCEP"; then
  echo "$as_me:$LINENO: result: $MGCEP" >&5
echo "${ECHO_T}$MGCEP" >&6
else
  echo "$as_me:$LINENO: result: no" >&5
echo "${ECHO_T}no" >&6
fi

	if test ! -f "$MGCEP"; then
		{ { echo "$as_me:$LINENO: error: Can't find mgcep of SPTK" >&5
echo "$as_me: error: Can't find mgcep of SPTK" >&2;}
   { (exit 1); exit 1; }; }
	fi
	version=`$MGCEP -h 2>&1 | grep "version" | sed -e "s/.* //g"`
	if test -z "$version"; then
		{ { echo "$as_me:$LINENO: error: mgcep in SPTK version 3.2 or later is required." >&5
echo "$as_me: error: mgcep in SPTK version 3.2 or later is required." >&2;}
   { (exit 1); exit 1; }; }
	elif test `echo "$version < 3.2" | $BC` = 1; then
		{ { echo "$as_me:$LINENO: error: mgcep in SPTK version 3.2 or later is required." >&5
echo "$as_me: error: mgcep in SPTK version 3.2 or later is required." >&2;}
   { (exit 1); exit 1; }; }
	fi
fi

