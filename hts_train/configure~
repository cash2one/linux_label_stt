  yes)
    BYTESWAP="1" ;;
  no)
    BYTESWAP="0" ;;
  *)
    { { echo "$as_me:$LINENO: error: Can't determine endianness" >&5
echo "$as_me: error: Can't determine endianness" >&2;}
   { (exit 1); exit 1; }; } ;;
esac





